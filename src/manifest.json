{
    "package": "com.application.demo",
    "name": "lowbee",
    "versionName": "1.0.0",
    "versionCode": "1",
    "minPlatformVersion": "1020",
    "icon": "/Common/logo.png",
    "features": [
        {
            "name": "system.prompt"
        },
        {
            "name": "system.router"
        },
        {
            "name": "system.shortcut"
        },
        { "name": "system.webview" }
    ],
    "permissions": [
        {
            "origin": "*"
        }
    ],
    "config": {
        "logLevel": "log"
    },
    "router": {
        "entry": "View/Pages/Containor",
        "pages": {
            "View/Pages/Containor": {
                "component": "containor"
            }
        }
    },
    "display": {
        "titleBarBackgroundColor": "#f2f2f2",
        "titleBarTextColor": "#414141",
        "fullScreen": true,
        "titleBar": false,
        "menu": false,
        "pages": {
            "View/Pages/Containor": {
                "Demo": {
                    "titleBarText": "示例页",
                    "menu": false
                }
            }
        }
    }
}
